    "/html/body/form/table[2]/tbody/tr[14]/td[1]/font/font": "Grading Method",
    "/html/body/form/table[2]/tbody/tr[14]/td[2]/pre/font[1]/font": "Intermediate Exam 45%, Final Exam 45%: (Short Answer + Essay)",
    "/html/body/form/table[2]/tbody/tr[14]/td[2]/pre/font[3]/font": "\r\n10% scrap and report",
    "/html/body/form/table[2]/tbody/tr[14]/td[2]/pre/font[5]/font": "\r\nSanitary medicine",
    "/html/body/form/table[2]/tbody/tr[15]/td[1]/font/font": "Textbooks and References",
    "/html/body/form/table[2]/tbody/tr[15]/td[2]/pre/font[1]/font": "There is no bibliography, but the references vary.",
